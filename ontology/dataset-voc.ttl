<http://vocab.fairdatacollective.org/gdmt/> a skos:ConceptScheme;
  dct:creator <http://orcid.org/0000-0002-9381-9693>, <http://orcid.org/0000-0003-4124-9040>;
  dct:description "Ontology containing base RDF properties and controlled terms used in the generic dataset metadata template."@en;
  dct:title "Ontology for Generic Dataset Metadata Template"@en;
  pav:createdOn "2020-12-01T00:00:00+01:00"@en;
  pav:lastUpdatedOn "2020-12-01T00:00:00+01:00"@en;
  pav:version "0.1"@en;
  skos:hasTopConcept gdmt:Economics, gdmt:OperationMaintenance, gdmt:Siting, gdmt:WindPowerPlant,
    gdmt:WindTurbine;
  skos:prefLabel "Ontology for Generic Dataset Metadata Template"@en .

gdmt-property:ResourceType a rdf:Property;
  rdfs:label "ResourceType"@en;
  skos:definition "Information about the type of the resource being described with metadata."@en;
  skos:prefLabel "ResourceType"@en .

gdmt-property:DatasetIdentifier a rdf:Property;
  rdfs:label "DatasetIdentifier"@en;
  skos:definition "Information about the globally unique and persistent identifier used to identify and optionally access (meta)data of the dataset being described."@en;
  skos:prefLabel "DatasetIdentifier"@en .

gdmt-property:RelatedResources a rdf:Property;
  rdfs:label "RelatedResources"@en;
  skos:definition "Information about resource related to the dataset or other entity being described."@en;
  skos:prefLabel "RelatedResources"@en .

gdmt-property:Version a rdf:Property;
  rdfs:label "Version"@en;
  skos:definition "Version of the resource being described."@en;
  skos:prefLabel "Version"@en .

gdmt-property:Language a rdf:Property;
  rdfs:label "Language"@en;
  skos:definition "Language in which the dataset being described is provided."@en;
  skos:prefLabel "Language"@en .

gdmt-property:Title a rdf:Property;
  rdfs:label "Title"@en;
  skos:definition "A name or title by which the dataset being described is known."@en;
  skos:prefLabel "Title"@en .

gdmt-property:Description a rdf:Property;
  rdfs:label "Description"@en;
  skos:definition "Detailed information (typically at the level of an abstract) about the dataset being described."@en;
  skos:prefLabel "Description"@en .

gdmt-property:Subject a rdf:Property;
  rdfs:label "Subject"@en;
  skos:definition "Concepts (keywords, classification, or free text terms) that define the dataset or purpose (subjects which can be addressed) using the dataset."@en;
  skos:prefLabel "Subject"@en .

gdmt-property:ControlledTerms a rdf:Property;
  rdfs:label "ControlledTerms"@en;
  skos:definition "Concepts that define the dataset or purpose sourced from controlled vocabulary."@en;
  skos:prefLabel "ControlledTerms"@en .

gdmt-property:Creator a rdf:Property;
  rdfs:label "Creator"@en;
  skos:definition "An entity that brought into existence the dataset being described. Creators can be people, organizations and/or physical or virtual infrastructure (e.g., sensors, software)."@en;
  skos:prefLabel "Creator"@en .

gdmt-property:CreatorName a rdf:Property;
  rdfs:label "CreatorName"@en;
  skos:definition "Information about the creator name."@en;
  skos:prefLabel "CreatorName"@en .

gdmt-property:CreatorIdentifier a rdf:Property;
  rdfs:label "CreatorIdentifier"@en;
  skos:definition "Information about the creator identifier."@en;
  skos:prefLabel "CreatorIdentifier"@en .

gdmt-property:CreatorAffiliation a rdf:Property;
  rdfs:label "CreatorAffiliation"@en;
  skos:definition "Information about the creator affiliations."@en;
  skos:prefLabel "CreatorAffiliation"@en .

gdmt-property:Contributor a rdf:Property;
  rdfs:label "Contributor"@en;
  skos:definition "An entity that brought into existence the dataset being described. Contributors can be people, organizations and/or physical or virtual infrastructure (e.g., sensors, software)."@en;
  skos:prefLabel "Contributor"@en .

gdmt-property:ContributorName a rdf:Property;
  rdfs:label "ContributorName"@en;
  skos:definition "Information about the contributor name."@en;
  skos:prefLabel "ContributorName"@en .

gdmt-property:ContributorIdentifier a rdf:Property;
  rdfs:label "ContributorIdentifier"@en;
  skos:definition "Information about the contributor identifier."@en;
  skos:prefLabel "ContributorIdentifier"@en .

gdmt-property:ContributorAffiliation a rdf:Property;
  rdfs:label "ContributorAffiliation"@en;
  skos:definition "Information about the contributor affiliations."@en;
  skos:prefLabel "ContributorAffiliation"@en .

gdmt-property:Publisher a rdf:Property;
  rdfs:label "Publisher"@en;
  skos:definition "The entity that plays the principal role in officially making the dataset public. The role can be to hold, archive, publish, print, distribute, release, issue, or produce the dataset being described. This property is often used to formulate the citation, so consider the prominence of the role and of the entity. For datasets, use the primary repository for serving the data, or the primary organization taking responsibility for serving the data, as the Publisher. The Distribution element can be used to describe the entity(ies) that make the dataset accessible."@en;
  skos:prefLabel "Publisher"@en .

gdmt-property:PublisherIdentifier a rdf:Property;
  rdfs:label "PublisherIdentifier"@en;
  skos:definition "Information about the publisher identifier."@en;
  skos:prefLabel "PublisherIdentifier"@en .

gdmt-property:Rights a rdf:Property;
  rdfs:label "Rights"@en;
  skos:definition "Ways in which the dataset may or may not be accessed and used."@en;
  skos:prefLabel "Rights"@en .

gdmt-property:LicenseIdentifier a rdf:Property;
  rdfs:label "LicenseIdentifier"@en;
  skos:definition "Information about the license identifier."@en;
  skos:prefLabel "LicenseIdentifier"@en .

gdmt-property:Date a rdf:Property;
  rdfs:label "Date"@en;
  skos:definition "Relevant dates related to the resource being described. One instance of date must be of datsetDateType=Published."@en;
  skos:prefLabel "Date"@en .

gdmt-property:Distribution a rdf:Property;
  rdfs:label "Distribution"@en;
  skos:definition "Details about the distribution for this individual public presentation of the dataset (if part of the Distribution metadata), or for all the public presentations of the dataset being described (if part of the Dataset metadata)."@en;
  skos:prefLabel "Distribution"@en .

gdmt-property:DistributionAccess a rdf:Property;
  rdfs:label "DistributionAccess"@en;
  skos:definition "Details about the distribution access."@en;
  skos:prefLabel "DistributionAccess"@en .

gdmt-property:Distributor a rdf:Property;
  rdfs:label "Distributor"@en;
  skos:definition "Information about this distributor of the dataset. The Distributor includes primarily the system doing the distribution, but can include the person or persons responsible as well, for example the person who reformsts the dataset for this distribution."@en;
  skos:prefLabel "Distributor"@en .

gdmt-property:DistributorName a rdf:Property;
  rdfs:label "DistributorName"@en;
  skos:definition "Information about the distributor name."@en;
  skos:prefLabel "DistributorName"@en .

gdmt-property:DistributorIdentifier a rdf:Property;
  rdfs:label "DistributorIdentifier"@en;
  skos:definition "Information about the distributor identifier."@en;
  skos:prefLabel "DistributorIdentifier"@en .

gdmt-property:DistributorAffiliation a rdf:Property;
  rdfs:label "DistributorAffiliation"@en;
  skos:definition "Information about the distributor affiliations."@en;
  skos:prefLabel "DistributorAffiliation"@en .

gdmt-property:Content a rdf:Property;
  rdfs:label "Content"@en;
  skos:definition "The specific entities that constitute the dataset being described, and the organization and provenance of those entities. (The dataset is defined as coming from a single Data Source.)"@en;
  skos:prefLabel "Content"@en .

gdmt-property:DataStream a rdf:Property;
  rdfs:label "DataStream"@en;
  skos:definition "A collection of one or more variable records originating from a single data source (e.g., sensor, process, or person). A data stream can grow over time."@en;
  skos:prefLabel "DataStream"@en .

gdmt-property:DataSource a rdf:Property;
  rdfs:label "DataSource"@en;
  skos:definition "Physical or conceptual entity that creates the data streams that make up the described dataset. The data source may or may not be associated with a fixed location; for example, a series of discrete sensors deployed over time to a single location may be considered a single data source in some systems."@en;
  skos:prefLabel "DataSource"@en .

gdmt-property:Variable a rdf:Property;
  rdfs:label "Variable"@en;
  skos:definition "A single entity being measured or computed."@en;
  skos:prefLabel "Variable"@en .

gdmt-property:SpatialCoverage a rdf:Property;
  rdfs:label "SpatialCoverage"@en;
  skos:definition "The geospatial area (region on Earth) covered by the dataset being described."@en;
  skos:prefLabel "SpatialCoverage"@en .

gdmt-property:SpatialCoveragePoint a rdf:Property;
  rdfs:label "SpatialCoveragePoint"@en;
  skos:definition "The geospatial coverage represented as a single point."@en;
  skos:prefLabel "SpatialCoveragePoint"@en .

gdmt-property:SpatialCoverageBoundingBox a rdf:Property;
  rdfs:label "SpatialCoverageBoundingBox"@en;
  skos:definition "The geospatial coverage represented using bounding box."@en;
  skos:prefLabel "SpatialCoverageBoundingBox"@en .

gdmt-property:SpatialCoveragePolygon a rdf:Property;
  rdfs:label "SpatialCoveragePolygon"@en;
  skos:definition "The geospatial coverage represented using polygon."@en;
  skos:prefLabel "SpatialCoveragePolygon"@en .

gdmt-property:VerticalCoverage a rdf:Property;
  rdfs:label "VerticalCoverage"@en;
  skos:definition "The vertical area (altitude and/or depth) covered by the dataset being described."@en;
  skos:prefLabel "VerticalCoverage"@en .

gdmt-property:TemporalCoverage a rdf:Property;
  rdfs:label "TemporalCoverage"@en;
  skos:definition "The temporal coverage and resolution of the dataset being described."@en;
  skos:prefLabel "TemporalCoverage"@en .

gdmt-property:Funding a rdf:Property;
  rdfs:label "Funding"@en;
  skos:definition "The sources of funding that enabled and drove the creation of the dataset. (Non-monetary sources are described under Contributor.)"@en;
  skos:prefLabel "Funding"@en .

gdmt-property:Award a rdf:Property;
  rdfs:label "Award"@en;
  skos:definition "Information about the project award."@en;
  skos:prefLabel "Award"@en .

gdmt-property:Funder a rdf:Property;
  rdfs:label "Funder"@en;
  skos:definition "Information about the award funder."@en;
  skos:prefLabel "Funder"@en .